{
  "version": "0.0.6-alpha.39",
  "name": "@homebase-id/js-lib",
  "author": "YouFoundation",
  "description": "Monorepo that contains all the main homebase apps, together with the JavaScript libraries",
  "files": [
    "dist",
    "core.js",
    "core.d.ts",
    "media.js",
    "media.d.ts",
    "helpers.js",
    "helpers.d.ts",
    "network.js",
    "network.d.ts",
    "profile.js",
    "profile.d.ts",
    "public.js",
    "public.d.ts",
    "peer.js",
    "peer.d.ts",
    "auth.js",
    "auth.d.ts"
  ],
  "type": "module",
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "exports": {
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./media": {
      "import": "./dist/media.js",
      "require": "./dist/media.cjs"
    },
    "./helpers": {
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.cjs"
    },
    "./network": {
      "import": "./dist/network.js",
      "require": "./dist/network.cjs"
    },
    "./profile": {
      "import": "./dist/profile.js",
      "require": "./dist/profile.cjs"
    },
    "./public": {
      "import": "./dist/public.js",
      "require": "./dist/public.cjs"
    },
    "./peer": {
      "import": "./dist/peer.js",
      "require": "./dist/peer.cjs"
    },
    "./auth": {
      "import": "./dist/auth.js",
      "require": "./dist/auth.cjs"
    }
  },
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "vite build --watch",
    "build": "tsc && vite build",
    "test": "vitest",
    "lint": "eslint src/*"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/youfoundation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/homebase-id/odin-js"
  },
  "dependencies": {
    "guid-typescript": "^1.0.9"
  },
  "optionalDependencies": {
    "@ffmpeg/util": "^0.12.1",
    "@homebase-id/ffmpeg": "0.12.15",
    "mp4box": "^0.5.2"
  },
  "devDependencies": {
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "axios": "^1.7.5"
  }
}
