  "name": "@youfoundation/dotyoucore-transit-lib",
  "author": "YouFoundation",
  "description": "Lib for using Youverse transit and drive WebAPI",
  "repository": {
    "type": "git",
    "url": "https://github.com/YouFoundation/dotyoucore-transit-lib.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/youfoundation"
  },
  "module": "dist/dotyoucore-transit-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/dotyoucore-transit-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dotyoucore-transit-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.25.0",
